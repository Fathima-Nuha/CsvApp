 <div class="m-4">
<h1 class="text-center">Upload CSV File</h1>

<div class="d-flex justify-content-center align-content-center">

<%= form_tag(file_upload_path, multipart: true) do %>
<div>

  <label class="mb-3" for="csv_file">Choose a CSV file:</label>
  </div>
<div class="mb-3">

  <%= file_field_tag 'csv_file' %>
</div>
<div >

  <%=submit_tag 'Upload', class:"btn btn-primary " %>

</div>

<% end %>

</div>



</div>
<div class="d-flex justify-content-center align-content-center ", style="margin-top: 5%;">

 <%=button_to 'Go back home',root_path,method: :get, class:"btn btn-primary " %>
</div>
<% flash.each do |type, message| %>
  <div class="alert alert-<%= type %>">
    <%= message %>
  </div>
<% end %>